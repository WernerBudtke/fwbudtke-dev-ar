{
  "name": "fwbudtke-dev-ar",
  "version": "0.1.0",
  "private": true,
  "description": "FWBudtke Dev Argentina Portfolio",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node ./scripts/generate-spa-pages.js",
    "preview": "vite preview --port 5173",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "test:ci": "vitest --run --reporter dot --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md,json}\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.141.6",
    "@tanstack/react-router-devtools": "^1.141.6",
    "daisyui": "^5.5.14",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.5.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/router-plugin": "^1.141.7",
    "@testing-library/dom": "^10.0.0",
